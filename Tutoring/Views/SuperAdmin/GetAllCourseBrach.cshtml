@model IEnumerable<DAL.Entities.tbl_course_branch>

@{
    ViewBag.Title = "GetAllCourseBrach";
    Layout = "~/Views/Shared/_superAdminLayout.cshtml";
}

<h2>GetAllCourseBrach</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.branchname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cr_date)
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.data) {
    <tr>
        <td>
            @item.branchname
        </td>
        <td>
            @item.cr_date
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.coursebranchid }) |
            @Html.ActionLink("Details", "Details", new { id=item.coursebranchid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.coursebranchid })
        </td>
    </tr>
}

</table>
